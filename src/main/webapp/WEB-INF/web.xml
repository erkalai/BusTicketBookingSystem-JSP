<?xml version="1.0" encoding="UTF-8"?>
<web-app
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
id="WebApp_ID"
version="4.0">

  <display-name>BusTicketingBooking-JSP</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>com.dev.search.SearchServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern> <!-- Make sure this matches -->
    </servlet-mapping>
    
    
     <servlet>
        <servlet-name>SearchBus</servlet-name>
        <servlet-class>com.dev.searchBus.SearchBus</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SearchBus</servlet-name>
        <url-pattern>/SearchBus</url-pattern> <!-- Make sure this matches -->
    </servlet-mapping>
  
  
</web-app>